   "source": [
    "def not_poor(str1):\n",
    "    snot = str1.find(\"not\")\n",
    "    spoor = str1.find(\"poor\")\n",
    "    \n",
    "    if spoor > snot and snot>0 and spoor>0:\n",
    "        str1 = str1.replace(str1[snot:(spoor+4)], 'good')\n",
    "        return str1\n",
    "    else:\n",
    "        return str1\n",
    "    \n",
    "print(not_poor(\"The food is not that poor!\"))"
   ]
